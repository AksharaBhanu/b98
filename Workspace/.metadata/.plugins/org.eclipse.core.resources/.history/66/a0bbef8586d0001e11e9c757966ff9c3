package day1;

import org.openqa.selenium.chrome.ChromeDriver;

public class Demo2 {

	public static void main(String[] args) {
		//creating object of ChromeDriver class 
		ChromeDriver c=new ChromeDriver();
		//calling non-static method
		c.get("http://www.google.com");
		//calling non-static method
		c.quit();

	}

}
