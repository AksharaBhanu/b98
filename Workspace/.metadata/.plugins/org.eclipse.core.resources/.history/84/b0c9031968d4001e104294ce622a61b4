package w;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;

class Emp implements Comparable<Emp>
{
	String name;
	
	Emp(String name)
	{
		this.name=name;
	}
	
	public String toString()
	{
		return name;
	}

	
	public int compareTo(Emp e2) {//DNSO
		String name1=this.name.toLowerCase();
		String name2=e2.name.toLowerCase();
		return name1.compareTo(name2);//A to Z
	}
}

class EmpSorting implements Comparator<Emp>
{


	public int compare(Emp e1, Emp e2) {
		String name1=e1.name.toLowerCase();
		String name2=e2.name.toLowerCase();
		return -name1.compareTo(name2);//Z to A
	}
	
}
